N = input("Enter Number of Samples: ");
x = input("Enter Lower Cutoff Frequency: ");
y = input("Enter Upper Cutoff Frequency: ");
figure(1);
s1 = fir1(N, [x, y], hamming(N+1));
freqz(s1, 1, 2000);
title("Hamming");
figure(2);
s1 = fir1(N, [x, y], hanning(N+1));
freqz(s1, 1, 2000);
title("Hanning");
figure(3);
s1 = fir1(N, [x, y], blackman(N+1));
freqz(s1, 1, 2000);
title("Blackman");
figure(4);
s1 = fir1(N, [x, y], rectwin(N+1));
freqz(s1, 1, 2000);
title("rectangle");
